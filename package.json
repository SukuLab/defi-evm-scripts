{
	"name": "@suku/defi-evm-scripts",
	"version": "0.2.3",
	"description": "A collection of scripts to run various tasks on Ethereum based defi smart contracts written in Typescript.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"prepublish": "npm run build",
		"prebuild": "npm run typechain",
		"build": "tsc",
		"example": "ts-node ./src/example.ts",
		"typechain": "typechain --target=ethers-v5 --outDir './src/contracts/types' './src/contracts/abis/**/*.json'",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [
		"[\"CLI\"]"
	],
	"author": "Bryce Doganer",
	"license": "MIT",
	"dependencies": {
		"@ethersproject/bignumber": "^5.0.7",
		"@types/web3": "^1.2.2",
		"bignumber.js": "^9.0.0",
		"chalk": "^4.0.0",
		"clear": "^0.1.0",
		"configstore": "^5.0.1",
		"dotenv": "^8.2.0",
		"ethers": "^5.0.13",
		"figlet": "^1.3.0",
		"inquirer": "^7.1.0",
		"lodash": "^4.17.15",
		"minimist": "^1.2.5",
		"node-rest-client-promise": "^3.1.1",
		"simple-git": "^1.132.0",
		"web3": "^1.2.11"
	},
	"devDependencies": {
		"@typechain/ethers-v5": "^1.0.0",
		"@types/clear": "^0.1.0",
		"@types/figlet": "^1.2.0",
		"@types/inquirer": "^6.5.0",
		"ts-node": "^8.10.2",
		"tslint": "^6.1.1",
		"typechain": "^2.0.0",
		"typescript": "^3.8.3"
	}
}
